{
  "name": "unbody",
  "version": "0.0.4",
  "main": "./index.js",
  "types": "./index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "tsconfig": "*",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "json-to-graphql-query": "^2.2.5",
    "lodash": "^4.17.21",
    "utility-types": "^3.11.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "transformIgnorePatterns": [
      "node_modules/"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "zod": "3.x.x"
  }
